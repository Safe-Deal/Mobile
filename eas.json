{
	"cli": {
		"version": ">= 7.6.2",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development"
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview"
		},
		"production": {
			"autoIncrement": true,
			"channel": "production"
		},
		"release": {
			"autoIncrement": true,
			"channel": "release"
		}
	},
	"submit": {
		"production": {}
	}
}
